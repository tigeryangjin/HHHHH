              A.GOODS_VIDEOURL,
              A.GOODS_STATE,
              A.GOODS_VERIFY,
              A.GOODS_ADDTIME,
              A.GOODS_EDITTIME,
              A.AREAID_1,
              A.AREAID_2,
              A.COLOR_ID,
              A.TRANSPORT_ID,
              A.GOODS_FREIGHT,
              A.GOODS_VAT,
              A.GOODS_COMMEND,
              A.GOODS_STCIDS,
              A.EVALUATION_GOOD_STAR,
              A.EVALUATION_COUNT,
              A.IS_VIRTUAL,
              A.VIRTUAL_INDATE,
              A.VIRTUAL_LIMIT,
              A.VIRTUAL_INVALID_REFUND,
              A.IS_FCODE,
              A.IS_APPOINT,
              A.IS_PRESELL,
              A.HAVE_GIFT,
              A.IS_OWN_SHOP,
              A.IS_TV,
              A.IS_ADD_CART,
              A.RETENTION_TIME,
