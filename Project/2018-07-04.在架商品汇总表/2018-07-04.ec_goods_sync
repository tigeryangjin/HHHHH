     S.IS_VIRTUAL,
     S.VIRTUAL_INDATE,
     S.VIRTUAL_LIMIT,
     S.VIRTUAL_INVALID_REFUND,
     S.IS_FCODE,
     S.IS_APPOINT,
     S.IS_PRESELL,
     S.HAVE_GIFT,
