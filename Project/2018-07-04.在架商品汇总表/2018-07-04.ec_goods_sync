     S.STORE_NAME,
     S.GC_ID,
     S.GC_ID_1,
     S.GC_ID_2,
     S.GC_ID_3,
     S.BRAND_ID,
     S.GOODS_PRICE,
     S.GOODS_PROMOTION_PRICE,
