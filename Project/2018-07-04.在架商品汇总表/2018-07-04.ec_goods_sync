         T.GOODS_PROMOTION_PRICE_3G  = S.GOODS_PROMOTION_PRICE_3G,
         T.GOODS_PROMOTION_TYPE_3G   = S.GOODS_PROMOTION_TYPE_3G,
         T.GOODS_PROMOTION_ID_3G     = S.GOODS_PROMOTION_ID_3G,
