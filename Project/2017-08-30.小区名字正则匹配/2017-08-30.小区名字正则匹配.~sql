UPDATE FACT_ECORDER_COMMON A
   SET A.DISTRICT_NAME = REGEXP_SUBSTR(A.RECIVER_INFO, '才子佳郡')
 WHERE (A.TRANSPZONE BETWEEN 4073 AND 4077 OR A.TRANSPZONE = 5954)
   AND A.DISTRICT_NAME IS NULL;
COMMIT;
