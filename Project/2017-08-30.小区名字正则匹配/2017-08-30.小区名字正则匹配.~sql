--create table district_list
create table district_list(district_name varchar2(100));
create table yj_district_tmp(district_name varchar2(100));
select * from yj_district_tmp for update;
insert into district_list
  select *
    from yj_district_tmp a
   where not exists (select 1
            from district_list b
           where a.district_name = b.district_name);
commit;


