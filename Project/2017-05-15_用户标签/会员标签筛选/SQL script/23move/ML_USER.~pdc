   COMMENT ON COLUMN "ML"."ML_USER"."AID" IS '管理员表自增id即管理员id';
 
   COMMENT ON COLUMN "ML"."ML_USER"."USERNAME" IS '管理员账号';
 
   COMMENT ON COLUMN "ML"."ML_USER"."PASSWORD" IS '管理员密码';
 
   COMMENT ON COLUMN "ML"."ML_USER"."LOGIN_TIME" IS '登录时间';
 
   COMMENT ON COLUMN "ML"."ML_USER"."UPDATE_TIME" IS '账户更新时间';
 
   COMMENT ON COLUMN "ML"."ML_USER"."CREATE_TIME" IS '账户创建时间';
 
   COMMENT ON COLUMN "ML"."ML_USER"."LOGIN_IP" IS '登录ip';
 
   COMMENT ON COLUMN "ML"."ML_USER"."RID" IS '角色id';
 
   COMMENT ON TABLE "ML"."ML_USER"  IS '管理员表';
