CREATE TABLE "ML"."ML_USER" 
   (	"AID" NUMBER(10,0) NOT NULL ENABLE, 
	"USERNAME" VARCHAR2(32) NOT NULL ENABLE, 
	"PASSWORD" VARCHAR2(32) NOT NULL ENABLE, 
	"LOGIN_TIME" DATE NOT NULL ENABLE, 
	"UPDATE_TIME" DATE NOT NULL ENABLE, 
	"CREATE_TIME" DATE NOT NULL ENABLE, 
	"LOGIN_IP" VARCHAR2(15), 
	"RID" NUMBER(10,0) DEFAULT '0', 
	 CONSTRAINT "ML_USER_PK" PRIMARY KEY ("AID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DB_DATA"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DB_DATA" ;
 
  CREATE UNIQUE INDEX "ML"."ML_USER_PK" ON "ML"."ML_USER" ("AID") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DB_DATA" ;
 
  ALTER TABLE "ML"."ML_USER" ADD CONSTRAINT "ML_USER_PK" PRIMARY KEY ("AID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "DB_DATA"  ENABLE;
 
