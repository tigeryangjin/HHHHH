CREATE TABLE "ML"."MEMBER_FILTER_RESULT" 
   (	"ROW_ID" NUMBER(10,0), 
	"FILTER_ID" NUMBER(10,0), 
	"MEMBER_BP" NUMBER(20,0), 
	"VID" VARCHAR2(100), 
	"OPENID" VARCHAR2(100)
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "DB_DATA" ;
 
   COMMENT ON COLUMN "ML"."MEMBER_FILTER_RESULT"."ROW_ID" IS '行记录ID，seq';
 
   COMMENT ON COLUMN "ML"."MEMBER_FILTER_RESULT"."FILTER_ID" IS '筛选ID';
 
   COMMENT ON COLUMN "ML"."MEMBER_FILTER_RESULT"."MEMBER_BP" IS '会员bp号';
 
   COMMENT ON COLUMN "ML"."MEMBER_FILTER_RESULT"."VID" IS '设备号';
 
   COMMENT ON COLUMN "ML"."MEMBER_FILTER_RESULT"."OPENID" IS 'openid';
 
   COMMENT ON TABLE "ML"."MEMBER_FILTER_RESULT"  IS '会员筛选结果表';
