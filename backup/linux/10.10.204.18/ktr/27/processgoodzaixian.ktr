    <sql>SELECT  a1.item_code,a1.goods_name,a1.gc_id_2,a1.gc_name,a1.goods_addtime,&#xa;CASE WHEN a1.is_tv&#x3d;0 THEN &#x27;&#x7535;&#x5546;&#x63d0;&#x62a5;&#x7ec4;&#x27;ELSE &#x27;&#x975e;&#x7535;&#x5546;&#x63d0;&#x62a5;&#x7ec4;&#x27; END AS  is_tv,a1.brand_name,a1.goods_price,&#xa;IFNULL&#x28;a2.matdlt,&#x27;UNKNOWN&#x27;&#x29; as matdlt,IFNULL&#x28;a2.matzlt,&#x27;UNKNOWN&#x27;&#x29; as matzlt,IFNULL&#x28;a2.matxlt,&#x27;UNKNOWN&#x27;&#x29; as matxlt,IFNULL&#x28;a2.matklt,&#x27;UNKNOWN&#x27;&#x29; as matklt,CAST&#x28;DATE_FORMAT&#x28;CURDATE&#x28;&#x29;, &#x27;&#x25;Y&#x25;m&#x25;d&#x27;&#x29; AS SIGNED &#x29; AS create_date_key,&#xa;CASE WHEN a1.is_shipping_self&#x3d;0 THEN &#x27;&#x4f9b;&#x5e94;&#x5546;&#x914d;&#x9001;&#x27;ELSE &#x27;&#x81ea;&#x9001;&#x27; END AS  is_shipping_self, a1.store_id,a1.store_name,a1.is_own_shop&#xa;FROM&#xa;&#x28;SELECT a.item_code,a.goods_name,a.gc_id_2,a.gc_name,a.store_id,a.store_name,&#xa;STR_TO_DATE&#x28;FROM_UNIXTIME&#x28;a.firstonselltime,&#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;H&#x3a;&#x25;i&#x3a;&#x25;s&#x27;&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;H&#x3a;&#x25;i&#x3a;&#x25;s&#x27;&#x29; AS goods_addtime, &#xa;a.matdl,a.matzl,a.matxl,a.matkl,a.is_tv,a.brand_id,a.brand_name,a.goods_price,&#xa;a.is_shipping_self,a.is_own_shop FROM  ec_goods_common a  WHERE a.goods_state&#x3d;1&#x29; a1 LEFT   JOIN ec_erp_mat a2&#xa;ON a1.matdl&#x3d;a2.matdl&#xa;AND   a1.matzl&#x3d;a2.matzl&#xa;AND   a1.matxl&#x3d;a2.matxl&#xa;AND   a1.matkl&#x3d;a2.matkl</sql>
