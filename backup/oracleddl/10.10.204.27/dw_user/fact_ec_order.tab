comment on column DW_USER.FACT_EC_ORDER.paymentchannel
  is '支付渠道，（支付宝手机:Alipay_M，支付宝网站Alipay_W，微信_WX）';
comment on column DW_USER.FACT_EC_ORDER.cust_no
  is '顾客编号';
comment on column DW_USER.FACT_EC_ORDER.addtime
  is '订单生成时间';
comment on column DW_USER.FACT_EC_ORDER.store_id
  is '卖家店铺id';
comment on column DW_USER.FACT_EC_ORDER.store_name
  is '卖家店铺名称';
