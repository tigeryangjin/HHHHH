CALL YANGJIN_PKG.ALTER_TABLE_SHRINK_SPACE('FACT_ORDER');
CALL YANGJIN_PKG.ALTER_TABLE_SHRINK_SPACE('DATA_ACQUISITION_ITEM_CURRENT');

SELECT blocks, empty_blocks, num_rows
FROM user_tables
WHERE table_name = 'FACT_ORDER';

