SELECT * FROM MLOG$_GOODS_LABEL_LINK;
SELECT * FROM MLOG$_MEMBER_LABEL_LINK;

insert into GOODS_LABEL_LINK
  (ITEM_CODE,
   G_LABEL_ID,
   G_LABEL_TYPE_ID,
   CREATE_DATE,
   CREATE_USER_ID,
   LAST_UPDATE_DATE,
   LAST_UPDATE_USER_ID,
   ROW_ID)
values
  (109652,
   22,
   1,
   to_date('23-04-2018 11:48:04', 'dd-mm-yyyy hh24:mi:ss'),
   'yangjin',
   to_date('31-05-2018 16:04:10', 'dd-mm-yyyy hh24:mi:ss'),
   'yangjin',
   109419);
COMMIT;

SELECT *
  FROM GOODS_LABEL_LINK A
 WHERE A.CREATE_USER_ID = 'yangjin'
   AND A.LAST_UPDATE_DATE >= DATE
 '2018-05-31'
   AND A.ROW_ID BETWEEN 109419 AND 109427;
	 
UPDATE GOODS_LABEL_LINK A
   SET A.LAST_UPDATE_DATE = SYSDATE
 WHERE A.CREATE_USER_ID = 'yangjin'
   AND A.LAST_UPDATE_DATE >= DATE
 '2018-05-31'
   AND A.ROW_ID BETWEEN 109419 AND 109427;
