--1.
SELECT *
  FROM S_PARAMETERS2 A
 WHERE A.PNAME LIKE 'YANGJIN_PKG.DATA_ACQUISITION%'
 ORDER BY A.PNAME;

--2.
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_ITEM_BASE'
 ORDER BY A.START_TIME DESC;

--3.
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_ITEM_CURRENT'
 ORDER BY A.START_TIME DESC;

--4.
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_ITEM_MIN_PER'
 ORDER BY A.START_TIME DESC;

--5. 
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_MONTH_NEW'
 ORDER BY A.START_TIME DESC;
 
--6. 
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_MONTH_TOPN'
 ORDER BY A.START_TIME DESC;
 
--7. 
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_WEEK_NEW'
 ORDER BY A.START_TIME DESC;
 
--8.
SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YANGJIN_PKG.DATA_ACQUISITION_WEEK_TOPN'
 ORDER BY A.START_TIME DESC;
 


SELECT *
  FROM ALL_SOURCE A
 WHERE UPPER(A.TEXT) LIKE '%DATA_ACQUISITION_ITEM_MIN_PER%';

 
SELECT A.PERIOD, COUNT(1)
  FROM DATA_ACQUISITION_ITEM_CURRENT A
 GROUP BY A.PERIOD;
