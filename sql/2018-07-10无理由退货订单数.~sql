SELECT *
  FROM FACT_GOODS_SALES_REVERSE A
 WHERE A.POSTING_DATE_KEY = 20180709;

SELECT * FROM DIM_RE_RESEAON_1;
SELECT * FROM DIM_RE_RESEAON_2;

SELECT * FROM DIM_RE_RESEAON_2 A WHERE A.REASON_NM LIKE '%无理由%';
