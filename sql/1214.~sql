SELECT * FROM MEMBER_ITEM_APRIORI_ITEMSET2 A ORDER BY A.BP_COUNT DESC;
SELECT A.ITEM_CODE_A,
       A.ITEM_CODE_B,
       A.BP_COUNT,
       CASE
         WHEN A.ITEM_CODE_A > A.ITEM_CODE_B THEN
          A.ITEM_CODE_B || '|' || A.ITEM_CODE_A
         ELSE
          A.ITEM_CODE_A || '|' || A.ITEM_CODE_B
       END || '|' || A.BP_COUNT
  FROM MEMBER_ITEM_APRIORI_ITEMSET2 A;
