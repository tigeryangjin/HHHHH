SELECT * FROM ALL_ALL_TABLES A WHERE A.owner = 'DW_USER';

SELECT A.ROW_WID,
       A.POSTING_DATE_KEY,
       A.MEMBER_KEY,
       A.ITEM_CODE,
       A.MATXL,
       A.PV_FREQ,
       A.FAV_FREQ,
       A.CAR_FREQ,
       A.ORDER_NET_QTY,
       A.PV_WEIGHT,
       A.FAV_WEIGHT,
       A.CAR_WEIGHT,
       A.ORDER_WEIGHT,
       A.W_INSERT_DT,
       A.W_UPDATE_DT
  FROM OPER_MEMBER_LIKE_BASE A, DIM_GOOD@DW_DATALINK B
 WHERE A.ITEM_CODE = B.ITEM_CODE;

SELECT * FROM DIM_GOOD@DW_DATALINK;
