  SELECT * FROM ODSHAPPIGO.OD_ORDER_ITEM A WHERE A.ORDER_NO = 20180525835210;
  SELECT * FROM ODSHAPPIGO.ODS_ORDER A WHERE A.CRM_OHGUID = 20180525835210;


SELECT A.ORDER_NO, A.ORDER_GB, A.ORDER_SEQ, COUNT(1)
  FROM ODSHAPPIGO.OD_ORDER_ITEM A
 GROUP BY A.ORDER_NO, A.ORDER_GB, A.ORDER_SEQ
HAVING COUNT(1) > 1;
