  from OPER_NM_PROMOTION_ITEM_REPORT t
 WHERE T.ORDER_SN = '630570483402375016';
select *
  from OPER_NM_PROMOTION_ORDER_REPORT t
 WHERE T.ORDER_SN = '630570483402375016';
select * from OPER_NM_VOUCHER_REPORT t where t.voucher_title = '129¼õ10';
select * from FACT_EC_ORDER_2 t WHERE T.ORDER_SN = '630570483402375016';
SELECT * FROM FACT_EC_P_MANSONG T WHERE T.MANSONG_ID = 235;
SELECT * FROM OPER_NM_VOUCHER_REPORT A WHERE A.voucher_title = '129¼õ10';


TRUNCATE TABLE OPER_NM_VOUCHER_REPORT;
begin
  -- Call the procedure
  yangjin_pkg.oper_nm_voucher_rpt;
end;
/

DECLARE IN_DATE_INT NUMBER(8);
IN_DATE DATE;
BEGIN_DATE DATE := DATE '2018-01-01';
END_DATE DATE := DATE '2018-03-07';
BEGIN
  IN_DATE := LAST_DAY(BEGIN_DATE);
  WHILE IN_DATE <= END_DATE LOOP
    IN_DATE_INT := TO_CHAR(IN_DATE, 'YYYYMMDD');
    YANGJIN_PKG.OPER_NM_PROMOTION_ITEM_RPT(IN_DATE_INT);
    YANGJIN_PKG.OPER_NM_PROMOTION_ORDER_RPT(IN_DATE_INT);
    IN_DATE := LAST_DAY(IN_DATE + 1);
  END LOOP;
END;
/
