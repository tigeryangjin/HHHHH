--Size  28.4 GB
ALTER TABLE MSEG ENABLE ROW MOVEMENT;
ALTER TABLE MSEG SHRINK SPACE;
ALTER TABLE MSEG DISABLE ROW MOVEMENT;
BEGIN
  DBMS_STATS.GATHER_TABLE_STATS(OWNNAME => 'ODSHAPPIGO',
                                TABNAME => 'MSEG',
                                DEGREE  => 4,
                                CASCADE => TRUE);
END;

SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'YJ_OPTIMIZATION.TABLE_OPTIMIZATION'
 ORDER BY A.START_TIME DESC;


