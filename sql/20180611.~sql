SELECT COUNT(DISTINCT A.VID) UV, COUNT(1) PV
  FROM FACT_PAGE_VIEW A
 WHERE A.VISIT_DATE_KEY BETWEEN 20180601 AND 20180610
   AND A.PAGE_VALUE = 'v:question0517app'
   AND A.PAGE_NAME = 'KFZT';
	 
	 
 SELECT DISTINCT A.PAGE_VALUE
   FROM FACT_PAGE_VIEW A
  WHERE A.VISIT_DATE_KEY BETWEEN 20180601 AND 20180610
    AND A.PAGE_VALUE LIKE '%question%'
    --AND A.PAGE_NAME = 'KFZT';
		
		
BEGIN
	CREATEFACTORDERGOODSREVERSE(20180530);
	PROCESSUPDATEORDERREVERSE(20180530);
END;
