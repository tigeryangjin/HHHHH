CREATE TABLE FACTEC_ORDER_STORE_TMP_YANGJIN AS
SELECT T.ORDER_ID,T.STORE_ID,T.STORE_NAME
FROM FACTEC_ORDER T
WHERE 1=2;

SELECT COUNT(1) FROM FACTEC_ORDER_STORE_TMP_YANGJIN;
TRUNCATE TABLE FACTEC_ORDER_STORE_TMP_YANGJIN;
--190490
SELECT MAX(ORDER_ID) FROM FACTEC_ORDER_STORE_TMP_YANGJIN;
SELECT * FROM FACTEC_ORDER_STORE_TMP_YANGJIN ORDER BY ORDER_ID;

UPDATE FACTEC_ORDER A
   SET (A.STORE_ID, A.STORE_NAME) =
       (SELECT B.STORE_ID, B.STORE_NAME
          FROM FACTEC_ORDER_STORE_TMP_YANGJIN B
         WHERE A.ORDER_ID = B.ORDER_ID)
 WHERE A.STORE_ID IS NULL
   AND EXISTS (SELECT 1
          FROM FACTEC_ORDER_STORE_TMP_YANGJIN C
         WHERE A.ORDER_ID = C.ORDER_ID);
				 
UPDATE FACTEC_ORDER A SET A.STORE_ID=1,A.STORE_NAME='øÏ¿÷π∫…Ã≥«' WHERE A.STORE_ID IS NULL;
COMMIT;
