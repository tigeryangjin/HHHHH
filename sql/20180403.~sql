--1.
SELECT A.Item_Code,
       SUM(A.EFFECTIVE_ORDER_AMOUNT) EFFECTIVE_ORDER_AMOUNT,
       SUM(A.EFFECTIVE_ORDER_QTY) EFFECTIVE_ORDER_QTY
  FROM OPER_PRODUCT_DAILY_REPORT A
 WHERE A.POSTING_DATE_KEY = 20180326
   AND A.SALES_SOURCE_SECOND_NAME = '新媒体APP（2.0）'
 GROUP BY A.Item_Code
 ORDER BY A.Item_Code;
--2.
SELECT *
  FROM OPER_PRODUCT_DAILY_REPORT A
 WHERE A.POSTING_DATE_KEY = 20180326
   AND A.SALES_SOURCE_SECOND_NAME = '新媒体APP（2.0）'
   AND A.ITEM_CODE = 230669;
--3.
SELECT A.ORDER_KEY,
       A.GOODS_COMMON_KEY,
       A.SALES_SOURCE_SECOND_DESC,
       A.ORDER_DATE_KEY,
       A.ORDER_STATE,
       A.NUMS,
       A.ORDER_AMOUNT,
       A.CANCEL_STATE,
       A.CANCEL_DATE_KEY,
       A.UPDATE_TIME
  FROM FACT_GOODS_SALES A
 WHERE A.SALES_SOURCE_SECOND_KEY = '20017'
   AND A.POSTING_DATE_KEY = 20180326
   AND A.GOODS_COMMON_KEY = 230669;
--4.
SELECT *
  FROM FACT_GOODS_SALES_REVERSE A
 WHERE A.SALES_SOURCE_SECOND_KEY = '20017'
   AND A.POSTING_DATE_KEY = 20180316
   AND A.GOODS_COMMON_KEY = 235272;
--5.

