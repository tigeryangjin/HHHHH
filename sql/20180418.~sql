DECLARE
  IN_DATE_INT NUMBER(8);
  IN_DATE     DATE;
  BEGIN_DATE  DATE := DATE '2018-04-01';
  END_DATE    DATE := DATE '2018-04-19';
BEGIN
  IN_DATE := BEGIN_DATE;
  WHILE IN_DATE <= END_DATE LOOP
    IN_DATE_INT := TO_CHAR(IN_DATE, 'YYYYMMDD');
    YANGJIN_PKG.OPER_TRAFFIC_ANALYSIS_PROC(IN_DATE_INT);
    YANGJIN_PKG.OPER_CLICK_ANALYSIS_PROC(IN_DATE_INT);
    IN_DATE := IN_DATE + 1;
  END LOOP;
END;
/
