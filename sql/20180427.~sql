UPDATE FACT_GOODS_SALES A
   SET A.ORDER_FROM =
       (SELECT C.ZCRMD018
          FROM YANGJIN.ORDER_FROM_TMP C
         WHERE A.ORDER_KEY = C.CRM_OBJ_ID)
 WHERE A.ORDER_FROM IS NULL
   AND EXISTS (SELECT 1
          FROM YANGJIN.ORDER_FROM_TMP D
         WHERE A.ORDER_KEY = D.CRM_OBJ_ID)
   AND A.ORDER_KEY = 5208024588;
COMMIT;

UPDATE FACT_ORDER A
   SET A.ORDER_FROM =
       (SELECT C.ZCRMD018
          FROM YANGJIN.ORDER_FROM_TMP C
         WHERE A.ORDER_KEY = C.CRM_OBJ_ID)
 WHERE A.ORDER_FROM IS NULL
   AND EXISTS (SELECT 1
          FROM YANGJIN.ORDER_FROM_TMP D
         WHERE A.ORDER_KEY = D.CRM_OBJ_ID)
   AND A.ORDER_KEY = 5208024588;
COMMIT;

SELECT * FROM YANGJIN.ORDER_FROM_TMP;
SELECT * FROM FACT_GOODS_SALES A WHERE A.ORDER_KEY = 5208024588;
SELECT * FROM FACT_ORDER A WHERE A.ORDER_KEY = 5208024588;
