SELECT * FROM OPER_NM_PROMOTION_ITEM_REPORT;
SELECT * FROM OPER_NM_PROMOTION_ORDER_REPORT;
SELECT * FROM OPER_NM_VOUCHER_REPORT;

/*
ORDER_SN=160560131269933794有三种促销，
*/
SELECT * FROM FACT_EC_ORDER_2 A WHERE A.ORDER_SN=160560131269933794;
SELECT * FROM FACT_EC_ORDER_GOODS A WHERE A.ORDER_ID=2330668;

SELECT * FROM OPER_NM_PROMOTION_ITEM_REPORT A WHERE A.ORDER_SN=160560131269933794;
SELECT * FROM OPER_NM_PROMOTION_ORDER_REPORT A WHERE A.ORDER_SN=160560131269933794;

SELECT DISTINCT A.PROMOTION_TYPE,A.PROMOTION_SOURCE FROM OPER_NM_PROMOTION_ITEM_REPORT A;

