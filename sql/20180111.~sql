SELECT *
  FROM W_ETL_LOG A
 WHERE A.PROC_NAME = 'MEMBER_REPURCHASE_PKG.MEMBER_REPURCHASE_TRACT_PROC'
 ORDER BY A.START_TIME DESC;
 
SELECT A.W_UPDATE_DT,COUNT(1) FROM OPER_REPURCHASE_MEMBER_TRACK A GROUP BY A.W_UPDATE_DT;
SELECT * FROM OPER_REPURCHASE_MEMBER_TRACK;
SELECT A.ITEM_CODE,COUNT(1) FROM OPER_REPURCHASE_MEMBER_TRACK A GROUP BY A.ITEM_CODE;
SELECT A.ITEM_CODE,COUNT(DISTINCT A.STOCK_QTY) FROM OPER_REPURCHASE_MEMBER_TRACK A GROUP BY A.ITEM_CODE HAVING COUNT(DISTINCT A.STOCK_QTY)>1;
SELECT * FROM OPER_REPURCHASE_MEMBER_TRACK A WHERE A.ITEM_CODE=224799;

SELECT * FROM OPER_PRODUCT_DAILY_REPORT A WHERE A.POSTING_DATE_KEY=20180112;
