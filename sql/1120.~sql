DECLARE
  ROW_BEGIN NUMBER := 1;
  ROW_END   NUMBER;
  MAX_ROW   NUMBER;
BEGIN
  SELECT COUNT(1) INTO MAX_ROW FROM MEMBER_LABEL_LINK;
  WHILE ROW_BEGIN < MAX_ROW LOOP
    ROW_END := ROW_BEGIN + 9999;
    DBMS_OUTPUT.put_line(MAX_ROW || ':' || ROW_BEGIN || '-' || ROW_END);
    INSERT INTO MEMBER_LABEL_LINK_LOSS
      SELECT *
        FROM MEMBER_LABEL_LINK A
       WHERE A.ROW_ID BETWEEN ROW_BEGIN AND ROW_END
         AND NOT EXISTS (SELECT 1
                FROM MEMBER_LABEL_LINK@ML18 B
               WHERE A.ROW_ID = B.ROW_ID);
    ROW_BEGIN := ROW_END + 1;
  END LOOP;
END;
/
