SELECT COUNT(1) FROM FACT_ECORDER_COMMON A WHERE (A.TRANSPZONE BETWEEN 4073 AND 4077 OR A.TRANSPZONE = 5954) AND A.DISTRICT_NAME IS NOT NULL;
