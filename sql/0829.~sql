SELECT A.GOODS_COMMON_KEY, MIN(A.POSTING_DATE_KEY)
  FROM FACT_GOODS_SALES A
 WHERE A.GOODS_COMMON_KEY IN
       (SELECT DISTINCT B.GOODS_KEY
          FROM OPER_PRODUCT_DAILY_REPORT B
         WHERE B.GOODS_NAME IS NULL)
 GROUP BY A.GOODS_COMMON_KEY
 ORDER BY 1;

SELECT A.GOODS_KEY, MIN(A.POSTING_DATE_KEY)
  FROM FACT_GOODS_SALES A
 WHERE A.GOODS_COMMON_KEY IN
       (SELECT DISTINCT B.GOODS_KEY
          FROM OPER_PRODUCT_DAILY_REPORT B
         WHERE B.GOODS_NAME IS NULL)
 GROUP BY A.GOODS_KEY
 ORDER BY 1;
 
SELECT A.GOODS_KEY, MIN(A.POSTING_DATE_KEY)
  FROM FACT_GOODS_SALES A
 WHERE A.GOODS_KEY IN
       (SELECT DISTINCT B.GOODS_KEY
          FROM OPER_PRODUCT_DAILY_REPORT B
         WHERE B.GOODS_NAME IS NULL)
 GROUP BY A.GOODS_KEY
 ORDER BY 1;
