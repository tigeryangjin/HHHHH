SELECT SUM(B.CNT) / COUNT(B.MEMBER_BP)
  FROM (SELECT A.MEMBER_BP, COUNT(1) CNT
          FROM MEMBER_ITEM_APRIORI_BASE A
         GROUP BY A.MEMBER_BP) B;

SELECT SUM(C.ROW2)
  FROM (SELECT B.MEMBER_BP, B.CNT, (B.CNT * (B.CNT - 1)) / 2 ROW2
          FROM (SELECT A.MEMBER_BP, COUNT(1) CNT
                  FROM MEMBER_ITEM_APRIORI_BASE A
                 GROUP BY A.MEMBER_BP) B) C;
