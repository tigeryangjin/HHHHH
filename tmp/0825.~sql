SELECT * FROM OPER_MBR_REG_WITHOUT_ORDER A WHERE A.RECORD_DATE_KEY=20170824;
SELECT * FROM OPER_MBR_ORDER_RETAIN_PUSH A WHERE A.RECORD_DATE_KEY=20170824;

SELECT A.MEMBER_KEY FROM OPER_MBR_REG_WITHOUT_ORDER A GROUP BY A.MEMBER_KEY HAVING COUNT(1)>1;

SELECT A.PROCESSED,COUNT(1) FROM OPER_MBR_REG_WITHOUT_ORDER A WHERE A.RECORD_DATE_KEY=20170824 GROUP BY A.PROCESSED;

SELECT * FROM OPER_MBR_REG_WITHOUT_ORDER A WHERE A.MEMBER_KEY=1615475092;

--224294, 224086, 224793, 224605, 224174, 224794
SELECT A.GOODS_COMMON_KEY, MIN(A.POSTING_DATE_KEY)
  FROM FACT_GOODS_SALES A
 WHERE A.GOODS_COMMON_KEY IN
       (224294, 224086, 224793, 224605, 224174, 224794)
 GROUP BY A.GOODS_COMMON_KEY;

SELECT *
  from dim_good a
 where a.item_code in (224294, 224086, 224793, 224605, 224174, 224794);

SELECT *
  from ods_zmaterial a
 where TO_NUMBER(a.zmater2) in
       (224294, 224086, 224793, 224605, 224174, 224794)
   and a.zmaterial not like '%F%'
   and a.ZEAMC027 is not null
   and a.zeamc027 != 0;


SELECT /*+PARALLEL(16)*/ *
  from ods_zmaterial a
 where a.zmater2 in
       ('000000000000224294', '000000000000224086', '000000000000224793', '000000000000224605', '000000000000224174', '000000000000224794')
   and a.zmaterial not like '%F%'
   --and a.ZEAMC027 is not null
   and a.zeamc027 != 0;
